<% include ./partials/header.ejs %>
<div class="w-full min-h-screen bg-zinc-800 px-10 py-8 text-white">
    <div class="cards flex flex-wrap gap-10">
        <% user.posts.forEach((elem) => { %>
        <div class="card h-48 bg-white rounded-lg shadow-zinc-400 bg-contain shadow-lg">
            <div class=" h-52 bg-sky-200 bg-auto rounded-lg overflow-hidden">
                <img src="/images/uploads/<%= elem.image %>" alt="" class="w-full h-full object-cover">
            </div>
        </div>
        <% }) %>
    </div>
</div>
<% include ./partials/footer.ejs %>
